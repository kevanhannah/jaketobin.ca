{
	"bracketSameLine": true,
	"endOfLine": "lf",
	"printWidth": 80,
	"singleQuote": true,
	"semi": true,
	"trailingComma": "es5",
	"useTabs": true,
	"plugins": ["prettier-plugin-svelte"],
	"pluginSearchDirs": ["."],
	"overrides": [
		{
			"files": "*.svelte",
			"options": {
				"parser": "svelte"
			}
		}
	]
}
